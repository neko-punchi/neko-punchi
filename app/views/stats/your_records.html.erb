<!--
<h1><％＝ current_user.name ％>さんの成績</h1>
-->
<table>
  <tr>
    <th>カテゴリ</th>
    <th>サブカテゴリ</th>
    <th>回答回数</th>
    <th>正答回数</th>
    <th>正答率</th>
  </tr>


<% @your_answer_rates.each do |line| %>
  <tr>
    <td><%= line[:category] %></td>
    <td><%= line[:sub_category] %></td>
    <td><%= line[:try_total] %></td>
    <td><%= line[:correct_sub] %></td>
    <td class="rate"><%= line[:rate] %></td>
  </tr>
<% end %>
</table>
